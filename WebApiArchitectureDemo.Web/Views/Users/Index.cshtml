@model List<WebApiArchitectureDemo.Common.DTOs.User>
@{
    ViewBag.Title = "All Users";
}

<h2>All Users</h2>

<table>
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Date of Birth
            </th>
            <th>
                Username
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.DateOfBirth.ToShortDateString()</td>
                <td>@item.Username</td>
            </tr>
        }
    </tbody>
</table>
